{
    "name": "Couch Voyager",
    "version": "1.0",
    "description": "Explore the world without leaving your seat!",
    "manifest_version": 2,
    "permissions": [
      "tabs"
    ],
    "content_scripts": [
      {
        "matches": [
          "*://*.google.com/maps/@*"
        ],
        "js": [
          "content.js"
        ]
      }
    ]
  }
  